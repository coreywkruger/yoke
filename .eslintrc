{
  "ecmaFeatures": {
    "es6": true,
    "modules": true,
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": [],
  "rules": {
    "camelcase": 0,
    "comma-spacing": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-restricted-modules": 1,
    "no-multi-spaces": 1,
    "no-redeclare": 1,
    "no-shadow": [1, {"builtinGlobals": false, "hoist": "all"}],
    "no-undef-init": 1,
    "no-undef": 1,
    "no-use-before-define": 1,
    "operator-assignment": 1,
    "quotes": [1, "single"],
    "semi": 1
  }
}

